  <connectionStrings>
    <add name="ReachContext" providerName="System.Data.SqlClient" connectionString="Data Source=(localdb)\v11.0;Initial Catalog=Reach;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\Reach.mdf" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
